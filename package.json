{
  "name": "@e-cloud/ngx-tree",
  "version": "0.2.1",
  "description": "A derived version of angular-tree-component without mobx. Much Better performance.",
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp",
    "start": "ng serve -o",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n ngx-tree -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "ng lint --type-check",
    "test": "ng test -sm=false",
    "release": "standard-version -a",
    "deploy": "npm run build && npm publish --access=public dist --registry=https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/e-cloud/ngx-tree"
  },
  "author": {
    "name": "e-cloud",
    "email": "saintscott119@gmail.com"
  },
  "keywords": [
    "angular"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/e-cloud/ngx-tree/issues"
  },
  "devDependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/cli": "^1.3.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/language-service": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@compodoc/compodoc": "^1.0.0-beta.9",
    "@types/jasmine": "^2.5.45",
    "@types/node": "~8.0.19",
    "classlist.js": "^1.1.20150312",
    "codelyzer": "~3.2.0",
    "core-js": "^2.4.1",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-rollup": "^2.14.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.5.3",
    "node-sass-tilde-importer": "^1.0.0",
    "protractor": "^5.1.2",
    "rollup": "^0.49.0",
    "run-sequence": "^2.0.0",
    "rxjs": "^5.4.0",
    "standard-version": "^4.1.0",
    "ts-node": "~3.3.0",
    "tslint": "~5.7.0",
    "typescript": "~2.5.0",
    "web-animations-js": "^2.2.5",
    "zone.js": "^0.8.4"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "standard-version": {
    "scripts": {
      "postbump": "node tools/bump-version.js && git add ."
    }
  },
  "dependencies": {
    "@types/lodash-es": "^4.14.6",
    "element-closest": "^2.0.2",
    "lodash-es": "^4.17.4"
  },
